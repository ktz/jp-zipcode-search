<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>jp-zipcode-search</application>
  <version>lift</version>
  <system-properties>
    <property name="in.gae.j" value="true"/>
    <property name="org.apache.commons.logging.Log" value="org.apache.commons.logging.impl.Log4JLogger"/>
    <property name="run.mode" value="production"/>
  </system-properties>
  <ssl-enabled>true</ssl-enabled>
  <sessions-enabled>true</sessions-enabled>
  <static-files>
    <!-- <include path="/images/*"/> -->
    <include path="/javascripts/**.js"/>
    <include path="/stylesheets/**.css" expiration="365d"/>
  </static-files>
</appengine-web-app>